{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "acacha/admin-lte-template-laravel": "4.*",
        "acacha/adminlte-laravel-installer": "~3.0",
        "backpack/backupmanager": "^1.2",
        "backpack/base": "^0.7.25",
        "backpack/crud": "^3.2",
        "backpack/langfilemanager": "^1.0",
        "backpack/logmanager": "^2.3",
        "backpack/pagemanager": "^1.1",
        "backpack/settings": "^2.0",
        "barryvdh/laravel-dompdf": "^0.8.1",
        "barryvdh/laravel-snappy": "^0.4.0",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "knplabs/knp-snappy": "^1.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "maatwebsite/excel": "~2.1.0",
        "owen-it/laravel-auditing": "^5.0",
        "predis/predis": "^1.1",
        "vsmoraes/laravel-pdf": "^2.0",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
